package persistentie.dao;

import domein.Gast;
import domein.Inschrijving;
import java.util.List;
import persistentie.GenericDao;

/**
 *
 * @author RootSoft
 * @param <T>
 */
public class GastDAO<T> extends GenericDao<Gast>{
    
    //Attributes
    
    //Constructors
    public GastDAO() {
        super(Gast.class);
    }

    //Callbacks
    
    @Override
    public List<Gast> findAll() {
        return super.findAll();
    }
    
    @Override
    public void insert(Gast g) {
        super.insert(g);
    }
    
    @Override
    public void update(Gast g) {
        super.update(g);
    }
    
    @Override
    public Gast get(int id) {
        return (Gast) super.get(id);
    }

}
