package persistentie.dao;

import domein.Campus;
import java.util.List;
import persistentie.GenericDao;

/**
 *
 * @author RootSoft
 * @param <T>
 */
public class CampusDAO<T> extends GenericDao<Campus>{
    
    //Attributes
    
    //Constructors
    public CampusDAO() {
        super(Campus.class);
    }

    //Callbacks
    
    @Override
    public List<Campus> findAll() {
        return super.findAll();
    }
    
    @Override
    public void insert(Campus c) {
        super.insert(c);
    }
    
    @Override
    public void update(Campus c) {
        super.update(c);
    }
    
    @Override
    public Campus get(int id) {
        return (Campus) super.get(id);
    }
    
}
